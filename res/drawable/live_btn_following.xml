        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
            <stroke android:width="0.6700001dip" android:color="@color/btn_confirm_press" />
            <corners android:radius="30.0dip" />
        </shape>
    </item>
    <item android:state_checked="false" android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/btn_confirm_press" />
            <corners android:radius="30.0dip" />/> </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
            <stroke android:width="0.6700001dip" android:color="@color/btn_confirm" />
            <corners android:radius="30.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/btn_confirm" />
            <stroke android:width="0.6700001dip" android:color="@color/btn_confirm" />
